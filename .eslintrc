{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 7
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "semi": ["error", "never"],
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "react/display-name": 1
  }
}